{
  "name": "storefront",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --config .eslintrc ./src --ext .ts",
    "format": "prettier --config .prettierrc src/**/*.ts --write",
    "jasmine": "jasmine",
    "lint:fix": "yarn lint -- --fix",
    "build": "rm -rf ./dist && tsc",
    "init-test-env": "env-cmd cross-var  \"yarn build && db-migrate  db:drop $Test_DB_Name && db-migrate db:create $Test_DB_Name && db-migrate -e test up\"",
    "run-test": "cross-env NODE_ENV=test yarn jasmine",
    "test2": "yarn init-test-env && yarn run-test",
    "watch": "concurrently \"yarn tsc --watch\" \"yarn nodemon ./dist/index.js\"",
    "migrate": "db-migrate up",
    "start": "node ./dist/index.js"
  },
  "dependencies": {
    "-": "^0.0.1",
    "D": "^1.0.0",
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "pg-format": "^1.0.4",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^4.0.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.35",
    "@types/pg": "^8.6.5",
    "@types/pg-format": "^1.0.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "concurrently": "^7.2.1",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine": "^4.1.0",
    "jasmine-spec-reporter": "^7.0.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3",
    "typescript": "^4.6.4"
  }
}
